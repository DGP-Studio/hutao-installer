{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Kachina Installer",
  "version": "0.1.0",
  "identifier": "click.kachina",
  "build": {
    "beforeDevCommand": "rsbuild dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "rsbuild build",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": false,
    "targets": "all",
    "icon": ["icons/icon.ico"]
  }
}
